# Example terraform.tfvars file
# Rename to terraform.tfvars and fill in your values

aws_region           = "us-east-1"
project_name         = "insoshi"
vpc_cidr             = "10.0.0.0/16"
availability_zones   = ["us-east-1a", "us-east-1b"]
key_name             = "your-key-pair-name"
db_name              = "insoshi"
db_username          = "dbadmin"
db_password          = "yourpassword"
multi_az_database    = true
web_server_capacity  = 2
instance_type        = "t3.micro"
db_instance_class    = "db.t3.small"
db_allocated_storage = 5
ssh_location         = "0.0.0.0/0"  # Restrict to your IP in production
ami_id               = "ami-0c55b159cbfafe1f0"  # Update with appropriate AMI for your region
